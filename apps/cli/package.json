{
  "name": "@webgal-tools/cli",
  "version": "1.7.4",
  "description": "WebGAL 工具集统一命令行接口",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "webgal-tools-cli": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/floatDreamWithSong/webgal-tools.git",
    "directory": "apps/cli"
  },
  "keywords": [
    "webgal",
    "cli",
    "mcp",
    "voice",
    "gaming"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "serve": "node dist/index.js",
    "prepublishOnly": "pnpm run build",
    "bundle": "node scripts/bundle.js"
  },
  "dependencies": {
    "@webgal-tools/config": "workspace:*",
    "@webgal-tools/mcp-server": "workspace:*",
    "@webgal-tools/voice": "workspace:*",
    "inquirer": "^10.1.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.0.0",
    "@vercel/ncc": "^0.38.3",
    "typescript": "^5.6.0"
  }
}