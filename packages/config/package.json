{
  "name": "@webgal-tools/config",
  "version": "1.6.10",
  "description": "统一的配置文件加载和管理",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "example/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/floatDreamWithSong/webgal-tools.git",
    "directory": "packages/config"
  },
  "keywords": [
    "webgal",
    "mcp",
    "config",
    "configuration"
  ],
  "scripts": {
    "build": "tsc && pnpm run copy-resources",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "copy-resources": "xcopy /E /I /Y example dist\\example"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.6.0",
    "@types/node": "^22.0.0"
  }
}